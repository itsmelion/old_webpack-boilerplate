{
    "name": "webpack-starter",
    "version": "0.0.1",
    "description": "A Generic configuration for scaffolding Web projects using ES6 or Typescript",
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=development webpack --progress --colors | PORT=8080 nodemon server.js",
        "express": "PORT=8080 node server.js",
        "serve": "webpack --progress --colors && npm run express",
        "build": "webpack -p --progress --colors",
        "hot": "NODE_ENV=hot webpack-dev-server --progress --colors --port=8080",
        "test": "karma start ./karma.conf.js",
        "deploy": "echo '\n\t######## WARN ########\n\tNo Deploy task specified\n\n'"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/alia-code/webpack.git"
