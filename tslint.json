{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            true,
            90
        ],
        "indent":true,
        "quotemark": false,
        "arrow-parens": false,
        "no-console": [
            false
        ],
        "only-arrow-functions": [
            true
        ],
        "no-var-requires": false,
        "ordered-imports": false
    },
    "rulesDirectory": [],
    "globals": {
        "$": false
    }
}